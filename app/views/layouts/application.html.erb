<!DOCTYPE html>
<html>
<head>
  <title>Personalizer</title>
  <link href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet">
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if notice && !notice.blank? %>
  <div id='flash_message' class='flash-notice'><%= notice %>
    <div class="close-notice" id='close_flash_message'>X</div>
  </div>
<% end %>

<% if alert && !alert.blank? %>
  <div id='flash_message' class='flash-alert'><%= alert %>
    <div class="close-notice" id='close_flash_message'>X</div>
  </div>
<% end %>


<div class="bg1"></div>
<svg class="bg2" viewBox="0 0 987.18375 987.18375">
